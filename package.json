{
  "name": "rocketotp",
  "version": "1.0.0",
  "description": "Rocket OTP Telegram bot",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf build && tsc",
    "start:dev": "nodemon",
    "start": "node build/index.js",
    "lint": "eslint . --ext .ts",
    "prettier-format": "run-script-os",
    "prettier-format:win32": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "prettier-format:darwin:linux": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-format:default": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "run-script-os",
    "prettier-watch:win32": "onchange \"src/**/*.ts\" -- prettier --write {{changed}}",
    "prettier-watch:darwin:linux": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "prettier-watch:default": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "test": "jest",
    "test:dev": "jest --watchAll"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run test && yarn run prettier-format && yarn run lint"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/coinbase-commerce-node": "^1.0.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@types/node-fetch": "^2.6.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.16",
    "onchange": "^7.1.0",
    "prettier": "^2.6.2",
    "run-script-os": "^1.1.6",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@messagebird/client": "^1.1.0",
    "@vonage/server-sdk": "^2.10.11",
    "axios": "^0.27.2",
    "coinbase-commerce-node": "^1.0.4",
    "contentful-management": "^10.3.0",
    "dotenv": "^16.0.0",
    "eslint-plugin-jest": "^26.1.5",
    "express": "^4.18.1",
    "jest": "^28.0.3",
    "jssip": "^3.9.0",
    "jssip-node-websocket": "^3.0.0",
    "messagebird": "^3.8.0",
    "moment": "^2.29.3",
    "plivo": "^4.31.0",
    "telegraf": "^4.8.1",
    "telegraf-session-local": "^2.1.0",
    "ts-jest": "^27.1.4",
    "uuid": "^8.3.2"
  }
}
